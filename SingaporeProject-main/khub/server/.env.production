# Production environment (z.com)
NODE_ENV=production
PORT=3000
JWT_SECRET=your_production_secret_here
MONGODB_URI=your_z_com_mongodb_uri
CORS_ORIGIN=https://your-frontend-domain.z.com

# Optional: Set these if using z.com's object storage for uploads
Z_STORAGE_KEY=your_z_storage_key
Z_STORAGE_SECRET=your_z_storage_secret
Z_STORAGE_BUCKET=your_bucket_name
Z_STORAGE_REGION=your_region